{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@database-url",
    "KOOP_API_URL": "https://onl-suggester.koop-innovatielab-tst.test5.s15m.nl/api/suggest",
    "ADMIN_USERNAME": "@admin-username",
    "ADMIN_PASSWORD": "@admin-password"
  }
}
